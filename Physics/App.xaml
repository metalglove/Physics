<Application x:Class="Physics.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Physics"
             xmlns:viewModels="clr-namespace:Physics.ViewModels"
             xmlns:views="clr-namespace:Physics.Views"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewModels:ProjectileLauncherViewModel}">
                <views:ProjectileLauncherView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
